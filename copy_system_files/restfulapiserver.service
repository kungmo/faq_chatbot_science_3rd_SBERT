[Unit]
Description = gunicorn daemon
After = network.target

[Service]
User = ubuntu
Group = ubuntu
WorkingDirectory = /home/ubuntu/faq_chatbot_science_3rd_SBERT
EnvironmentFile = /home/ubuntu/faq_chatbot_science_3rd_SBERT/environment_gunicorn.env
ExecStart = /usr/bin/gunicorn --workers=6 --bind=unix:/tmp/gunicorn.sock restfulapiserver.wsgi:application --timeout 300

[Install]
WantedBy = multi-user.target
